{
  "name": "app",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "templateInfo": {
    "name": "redux",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "build:swan": "taro build --type swan",
    "build:weapp": "taro build --type weapp",
    "build:weapp:prod": "cross-env LD_ENV=prod taro build --type weapp",
    "build:weapp:test": "cross-env LD_ENV=test taro build --type weapp",
    "build:weapp:local": "cross-env LD_ENV=local taro build --type weapp",
    "build:weapp:auto": "cross-env LD_ENV=auto-test taro build --type weapp",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:weapp": "npm run build:weapp -- --watch",
    "start:h5": "cross-env LD_ENV=local npm run build:h5 -- --watch",
    "start:weapp": "cross-env LD_ENV=local npm run build:weapp -- --watch",
    "mock:weapp": "cross-env LD_ENV=dev npm run build:weapp -- --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "lint-staged": {
    "ignore": [
      "node_modules/**"
    ],
    "linters": {
      "*.{ts,tsx}": [
        "eslint --fix",
        "prettier --write",
        "git add"
      ],
      "*.{js,jsx}": [
        "eslint --fix",
        "prettier --write",
        "git add"
      ],
      "*.less": [
        "stylelint --syntax less",
        "prettier --write",
        "git add"
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/cli": "3.1.4",
    "@tarojs/components": "3.1.4",
    "@tarojs/plugin-mock": "^0.0.6",
    "@tarojs/react": "3.1.4",
    "@tarojs/runtime": "3.1.4",
    "@tarojs/taro": "3.1.4",
    "@types/mockjs": "^1.0.2",
    "ahooks": "^2.10.0",
    "classnames": "^2.2.6",
    "dva-core": "^2.0.4",
    "dva-immer": "^0.4.19",
    "dva-loading": "^3.0.20",
    "dva-model-extend": "^0.1.2",
    "lodash": "^4.17.15",
    "mockjs": "^1.1.0",
    "path-to-regexp": "^6.1.0",
    "qs": "^6.9.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "taro-axios": "^1.1.1",
    "taro-ui": "^3.0.0-alpha.10"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@tarojs/mini-runner": "3.1.4",
    "@tarojs/webpack-runner": "3.1.4",
    "@types/classnames": "^2.2.10",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "babel-plugin-import": "^1.13.0",
    "babel-preset-taro": "3.1.4",
    "commitizen": "^4.0.3",
    "cross-env": "^7.0.2",
    "cspell": "^4.2.5",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-taro": "3.1.4",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-taro": "3.1.4",
    "husky": "^1.3.1",
    "lint-staged": "^7.2.2",
    "mp-ci": "^1.1.0",
    "prettier": "^1.19.1",
    "stylelint": "9.3.0",
    "taro-iconfont-cli": "^3.1.0",
    "typescript": "^4.1.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "yapi-to-typescript": "^3.14.0"
  }
}
